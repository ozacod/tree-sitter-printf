{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "printf",
  "rules": {
    "format_string": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "text_parts"
      }
    },
    "text_parts": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "text"
        },
        {
          "type": "SYMBOL",
          "name": "format"
        },
        {
          "type": "STRING",
          "value": "%%"
        },
        {
          "type": "SYMBOL",
          "name": "escape_sequence"
        }
      ]
    },
    "format": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "%"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "flags",
              "content": {
                "type": "SYMBOL",
                "name": "flags"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "width",
              "content": {
                "type": "SYMBOL",
                "name": "width"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "precision",
              "content": {
                "type": "SYMBOL",
                "name": "precision"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "size",
              "content": {
                "type": "SYMBOL",
                "name": "size"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "FIELD",
          "name": "type",
          "content": {
            "type": "SYMBOL",
            "name": "type"
          }
        }
      ]
    },
    "type": {
      "type": "PATTERN",
      "value": "[a-zA-Z]"
    },
    "flags": {
      "type": "PATTERN",
      "value": "[+\\-#0 ]+"
    },
    "width": {
      "type": "PATTERN",
      "value": "[1-9][0-9]*|\\*"
    },
    "precision": {
      "type": "PATTERN",
      "value": "\\.\\*|\\.[0-9]*"
    },
    "size": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "hh"
        },
        {
          "type": "STRING",
          "value": "h"
        },
        {
          "type": "STRING",
          "value": "j"
        },
        {
          "type": "STRING",
          "value": "l"
        },
        {
          "type": "STRING",
          "value": "L"
        },
        {
          "type": "STRING",
          "value": "ll"
        },
        {
          "type": "STRING",
          "value": "t"
        },
        {
          "type": "STRING",
          "value": "w"
        },
        {
          "type": "STRING",
          "value": "z"
        },
        {
          "type": "STRING",
          "value": "I"
        },
        {
          "type": "STRING",
          "value": "I32"
        },
        {
          "type": "STRING",
          "value": "I64"
        }
      ]
    },
    "escape_sequence": {
      "type": "PATTERN",
      "value": "\\\\[abfnrtv\\\\\\\\'\"?0]|\\\\x[0-9a-fA-F]+|\\\\[0-7]{1,3}|\\\\u[0-9a-fA-F]{4}|\\\\U[0-9a-fA-F]{8}"
    },
    "text": {
      "type": "PATTERN",
      "value": "[^%\\\\\\n]+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\n"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}